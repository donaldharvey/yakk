{
  "presets": [
    'react',
    ["env", {
      "targets": {
        "chrome": 56,
        "firefox": 54
      },
      "debug": true,
      "exclude": ["transform-regenerator"],
    }]
  ],
  "plugins": [
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
